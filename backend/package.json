{
  "name": "bookwormz",
  "version": "1.0.0",
  "description": "FULLSTACK book club app built on MERN stack",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cross-env NODE_ENV=development nodemon app",
    "client": "npm start --prefix bookwormz",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix bookwormz && npm run build --prefix bookwormz"
  },
  "keywords": [
    "express",
    "passport",
    "OAuth",
    "node",
    "mongo"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
        "body-parser": "^1.20.2",
        "colors": "^1.4.0",
        "connect-mongo": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.1",
        "mongo-connect": "^0.0.6",
        "mongoose": "^7.3.4",
        "morgan": "^1.10.0",
        "session": "^0.1.0"
  }
}
