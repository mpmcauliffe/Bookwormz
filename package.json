{
  "name": "bookwormz",
  "version": "1.0.0",
  "description": "FULLSTACK book club app built on MERN stack",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cross-env NODE_ENV=development nodemon app",
    "client": "npm start --prefix bookwormz",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix bookwormz && npm run build --prefix bookwormz"
  },
  "keywords": [
    "express",
    "passport",
    "OAuth",
    "node",
    "mongo"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongo-connect": "0.0.6",
    "mongoose": "^6.5.3",
    "morgan": "^1.10.0",
    "passport": "^0.6.0"
  }
}
